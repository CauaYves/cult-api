{
  "name": "dev",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc -p tsconfig.build.json && tscpaths -p tsconfig.build.json -s ./src -o ./dist && npx prisma migrate deploy --schema ./prisma/schema.prisma",
    "prepare": "husky install",
    "start": "node dist/server.js",
    "migration:run": "prisma migrate deploy",
    "migration:generate": "npm run dev:load-envs prisma migrate dev",
    "lint": "eslint .",
    "lint:staged": "lint-staged",
    "test": "cross-env NODE_ENV=test jest --passWithNoTests --runInBand",
    "test:coverage": "cross-env NODE_ENV=test jest --passWithNoTests --runInBand --coverage --collectCoverageFrom='./src/**'",
    "test:migration:run": "npm run test:load-envs prisma migrate deploy",
    "test:migration:generate": "npm run test:load-envs prisma migrate dev",
    "test:watch": "jest --watch --passWithNoTests --runInBand",
    "test:load-envs": "dotenv -e .env.test",
    "test:seed": "npm run test:load-envs prisma db seed",
    "dev:load-envs": "dotenv -e .env.development",
    "dev:migration:run": "npm run dev:load-envs prisma migrate deploy",
    "dev:migration:generate": "npm run dev:load-envs prisma migrate dev",
    "dev:seed": "npm run dev:load-envs prisma db seed",
    "dev": "tsx watch --env-file .env src/server.ts"
  },
  "author": "Caua Yves",
  "license": "MIT",
  "devDependencies": {
    "@faker-js/faker": "^6.3.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^20.10.3",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-boundaries": "^3.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "git-commit-msg-linter": "^4.1.2",
    "husky": "^8.0.1",
    "jest": "^26.6.3",
    "lint-staged": "^12.4.1",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "prisma": "^5.7.0",
    "supertest": "^6.2.3",
    "ts-jest": "^26.5.5",
    "tsconfig-paths": "3.9.0",
    "tscpaths": "^0.0.9",
    "tsx": "^4.7.0",
    "ttypescript": "^1.5.13",
    "typescript": "^4.2.4",
    "typescript-transform-paths": "2.2.3"
  },
  "dependencies": {
    "@brazilian-utils/brazilian-utils": "^1.0.0-rc.12",
    "@prisma/client": "^5.6.0",
    "@types/axios": "^0.14.0",
    "axios": "^1.1.3",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
