    "dotenv": "^16.0.1",
    "dotenv-expand": "^8.0.3",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "http-status": "^1.5.2",
    "ioredis": "^5.3.2",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "qs": "^6.11.2",
    "query-string": "^7.1.1",
